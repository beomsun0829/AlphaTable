{
    "name": "Let Tab",
    "description": "",
    "version": "1.0",
    "manifest_version": 3,
    "permissions":["storage", "tabs", "sidePanel"],
    "chrome_url_overrides":{
        "newtab": "index.html"
    },
    "action":{
        "default_popup": "src/components/popup/popup.html"
    },
    "side_panel":{
        "default_path": "src/components/sidepanel/sidepanel.html",
        "openPanelOnActionClick": true
    },
    "commands":{
        "_execute_action":{
            "suggested_key":{
                "default": "Alt + Q"
            },
            "description": "Open the extension popup"
        }
    }
}